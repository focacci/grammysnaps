{
  "scripts": {
    "tf:staging": "cd aws-infrastructure && terraform apply -var-file=terraform.staging.tfvars",
    "tf:production": "cd aws-infrastructure && terraform apply -var-file=terraform.production.tfvars",
    "tf:plan:staging": "cd aws-infrastructure && terraform plan -var-file=terraform.staging.tfvars",
    "tf:plan:production": "cd aws-infrastructure && terraform plan -var-file=terraform.production.tfvars",
    "tf:unlock": "cd aws-infrastructure && terraform force-unlock",
    "deploy:staging": "./scripts/deploy -e staging -b $(git branch --show-current)",
    "deploy:production": "./scripts/deploy -e production -b $(git branch --show-current)",
    "status": "./scripts/deployment-status"
  },
  "dependencies": {
    "canvas": "^3.1.2",
    "form-data": "^4.0.4",
    "node-fetch": "^2.7.0"
  },
  "version": "1.0.0"
}
