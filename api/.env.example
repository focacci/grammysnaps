# Test environment variables for local migration testing
# Copy this to .env in the api directory and update with your local database credentials

# Local PostgreSQL database for testing
DB_HOST=localhost
DB_PORT=5432
DB_NAME=grammysnaps_dev
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Development environment
NODE_ENV=development

# Other required variables (for testing purposes)
JWT_SECRET=test-jwt-secret-32-characters-minimum
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
S3_BUCKET_NAME=test-bucket
